{
  "url": "scripts\\.iucr\\.org",
    "elements" : {
        "authors": {
            "selector": "//a[contains(@href, 'author_name=')]",
            "attribute": "text"
        },
        "authorUrl": {
            "selector": "//a[contains(@href, 'author_name=')]",
            "attribute": "href"
        },
        "doi": {
            "selector": "//meta[@name='DC.identifier']",
            "attribute": "content"
        },
        "fulltext_pdf": {
            "selector": "//a[img[@alt='pdf version']]",
            "attribute": "href",
            "download": {
              "rename": "fulltext.pdf"
            }
        }
    }
}
