{
  "url": "tandfonline\\.com",
  "elements": {
    "publisher": {
      "selector": "//meta[@name='dc.Publisher']",
      "attribute": "content"
    },
    "title": {
      "selector": "//meta[@name='dc.Title']",
      "attribute": "content"
    },
    "authors": {
      "selector": "//meta[@name='dc.Creator']",
      "attribute": "content"
    },
    "date": {
      "selector": "//meta[@name='dc.Date']",
      "attribute": "content"
    },
    "pagerange": {
      "selector": "//div[@class='pageRange']",
      "attribute": "text"
    },
    "doi": {
      "selector": "//meta[@scheme='doi']",
      "attribute": "content"
    },
    "description": {
      "selector": "//meta[@name='dc.Description']",
      "attribute": "content"
    },
    "abstract": {
      "selector": "//meta[@class='dc.Description']",
      "attribute": "content"
    },
    "language": {
      "selector": "//meta[@name='dc.Language']",
      "attribute": "content"
    },
    "fulltext_pdf": {
      "selector": "//a[@class='pdf']",
      "attribute": "href",
      "download": true
    },
    "fulltext_html": {
      "selector": "//a[@class='txt']",
      "attribute": "href",
      "download": true
    },
    "supplementary_material": {
      "selector": "//a[@class='suppl']",
      "attribute": "href",
      "download": true
    }
  }
}
