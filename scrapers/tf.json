{
  "url": "http://www\\.tandfonline\\.com",
  "elements": {
    "fulltext_pdf": {
      "selector": "//a[@class='pdf last']",
      "attribute": "href",
      "download": {
        "rename": "fulltext.pdf"
      }
    },
    "fulltext_html": {
      "selector": "//div/a[@class='txt']",
      "attribute": "href",
      "download": {
        "rename": "fulltext.html"
      }
    },
    "csv1": {
      "selector": "//a[@id='CSVdownloadButton'][1]",
      "attribute": "href",
      "download": {
        "rename": "table1.csv"
      }
    },
    "csv2": {
      "selector": "//a[@id='CSVdownloadButton'][2]",
      "attribute": "href",
      "download": {
        "rename": "table2.csv"
      }
    },
    "csv3": {
      "selector": "//a[@id='CSVdownloadButton'][3]",
      "attribute": "href",
      "download": {
        "rename": "table3.csv"
      }
    },
    "csv4": {
      "selector": "//a[@id='CSVdownloadButton'][4]",
      "attribute": "href",
      "download": {
        "rename": "table4.csv"
      }
    },
    "csv5": {
      "selector": "//a[@id='CSVdownloadButton'][5]",
      "attribute": "href",
      "download": {
        "rename": "table5.csv"
      }
    },
    "csv6": {
      "selector": "//a[@id='CSVdownloadButton'][6]",
      "attribute": "href",
      "download": {
        "rename": "table6.csv"
      }
    },
    "title": {
      "selector": "//meta[@property='og:title']",
      "attribute": "content"
    },
    "author": {
      "selector": "//meta[@name='dc.Creator']",
      "attribute": "content"
    },
    "date": {
      "selector": "//meta[@name='dc.Type']",
      "attribute": "content"
    },
    "doi": {
      "selector": "//meta[@name='dc.Source']",
      "attribute": "content"
    },
    "volume": {
      "selector": "//meta[@name='citation_volume']",
      "attribute": "content"
    },
    "issue": {
      "selector": "//meta[@name='citation_issue']",
      "attribute": "content"
    },
    "firstpage": {
      "selector": "//meta[@name='citation_firstpage']",
      "attribute": "content"
    },
    "description": {
      "selector": "//meta[@name='description']",
      "attribute": "content"
    }
  }
}
