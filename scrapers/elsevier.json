{
  "url": "sciencedirect\\.com",
  "elements": {
    "title": {
      "selector": "//title",
      "attribute": "content"
    },
    "fulltext_html": {
      "selector": "//link[contains(@rel, 'canonical')]",
      "attribute": "href",
      "download": {
        "rename": "fulltext.html"
      }
    },

    "fulltext_pdf": {
      "selector": "//*[contains(@id, 'pdfLink')]",
      "attribute": "href",
      "download": {
        "rename": "fulltext.pdf"
      }
    }
  }
}

