{
  "url": "sciencedirect\\.com",
  "elements": {
    "title": {
      "selector": "/html/head/title",
      "attribute": "content"
    },
    "fulltext_html": {
      "selector": "//link[contains(@rel, 'canonical')]",
      "attribute": "href",
      "download": {
        "rename": "fulltext.html"
      }
    },

    "fulltext_pdf": {
      "selector": "(//*[@id='article-download']/a | //*[@id='pdfLink'])",
      "attribute": "href",
      "download": {
        "rename": "fulltext.pdf"
      }
    }
  }
}

