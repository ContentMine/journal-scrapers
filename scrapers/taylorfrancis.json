{
	"url": "tandfonline.com",
	"elements": {
		"publisher": {
      "selector": "//meta[@name='dc.Publisher']",
      "attribute": "content"
    },
    
    "journal_name": {
      "selector": "//meta[@name='citation_journal_title']",
      "attribute": "content"
    },
    
    "volume": {
      "selector": "//*[@id='unit2']/div[1]/div/div/table/tbody/tr/td[1]/h3/a[1]",
      "attribute": "content"
    },

    "issue": {
      "selector": "//*[@id='unit2']/div[1]/div/div/table/tbody/tr/td[1]/h3/a[2]",
      "attribute": "content"
    },

    "abstract": {
      "selector": "//meta[@name='dc.Description']",
      "attribute": "content",
      "download": {
        "rename": "abstract.html"
      }
    },

    "title": {
      "selector": "//meta[@name='dc.Title']",
      "attribute": "content"
    },

    "keywords": {
      "selector": "//meta[@name='dc.Subject']",
      "attribute": "text"
    },

    "author_name": {
      "selector": "//meta[@name='dc.Creator']",
      "attribute": "content"
    },

        "date_published": {
      "selector": "//meta[@name='dc.Date']",
      "attribute": "text"
    },

    "doi": {
      "selector": "//meta[@name='dc.Identifier']",
      "attribute": "content"
    },

    "csv1": {
      "selector": "//a[@id='CSVdownloadButton'][1]",
      "attribute": "href",
      "download": {
        "rename": "table1.csv"
      }
    },
    
    "csv2": {
      "selector": "//a[@id='CSVdownloadButton'][2]",
      "attribute": "href",
      "download": {
        "rename": "table2.csv"
      }
    },
    
    "csv3": {
      "selector": "//a[@id='CSVdownloadButton'][3]",
      "attribute": "href",
      "download": {
        "rename": "table3.csv"
      }
    },
    
    "csv4": {
      "selector": "//a[@id='CSVdownloadButton'][4]",
      "attribute": "href",
      "download": {
        "rename": "table4.csv"
      }
    },
    
    "csv5": {
      "selector": "//a[@id='CSVdownloadButton'][5]",
      "attribute": "href",
      "download": {
        "rename": "table5.csv"
      }
    },
    
    "csv6": {
      "selector": "//a[@id='CSVdownloadButton'][6]",
      "attribute": "href",
      "download": {
        "rename": "table6.csv"
      }
    },

    "fulltext_html": {
      "selector": "//meta[@name='dc.Source']",
      "attribute": "content",
      "download": {
        "rename": "fulltext.html"
      }
    },

    "fulltext_pdf": {
      "selector": "//a[text()='PDF']",
      "attribute": "href",
      "download": {
        "rename": "fulltext.pdf"
      }
    }

	}	
}